{
  "name": "moleculer-io",
  "version": "1.1.3",
  "description": "An API GateWay service for moleculerjs using socket.io",
  "main": "lib/index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "babel src -d lib --watch",
    "example": "npm install && node examples/simple/index.js",
    "test": "ava",
    "compile": "babel -d lib/ src/",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiaod/moleculer-io.git"
  },
  "keywords": [
    "socket.io",
    "moleculer",
    "microservice",
    "API Gateway"
  ],
  "author": "tiaod",
  "license": "MIT",
  "dependencies": {
    "chalk": "4.1.0",
    "lodash": "4.17.20",
    "socket.io": "2.3.0"
  },
  "devDependencies": {
    "@ava/babel": "1.0.1",
    "ava": "3.12.1",
    "babel-cli": "6.26.0",
    "babel-preset-env": "1.7.0",
    "express": "4.17.1",
    "fs-extra": "9.0.1",
    "moleculer": "0.14.10",
    "moleculer-web": "0.9.1",
    "socket.io-client": "2.3.0",
    "socket.io-redis": "5.4.0",
    "winston": "3.3.3"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": true
  }
}
